{
  "name": "PokeNurse",
  "version": "2.0.0",
  "description": "A tool for Pokémon Go to aid in transferring and evolving Pokémon",
  "main": "main.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production electron ./app/",
    "lint": "eslint --ignore-path .gitignore --format=node_modules/eslint-formatter-pretty app main *.js",
    "hot-dashboard": "yarn run hot-server -- --dashboard",
    "hot-server": "node --max_old_space_size=2096 -r babel-register server.js",
    "build-main": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.electron.js --progress --profile --colors",
    "build-renderer": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.production.js --progress --profile --colors",
    "build": "yarn run build-main && yarn run build-renderer",
    "start-hot": "cross-env HOT=1 NODE_ENV=development electron -r babel-register -r babel-polyfill ./app/main.development.js",
    "package": "npm run build && build --publish never",
    "package-win": "npm run build && build --win --x64",
    "package-linux": "npm run build && build --linux",
    "package-all": "npm run build && build -mwl --x64 --ia32",
    "dev": "yarn run hot-server -- --start-hot"
  },
  "build": {
    "appId": "com.vinnymac.PokeNurse",
    "category": "public.app-category.tools",
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "files": [
      "imgs/",
      "cries/",
      "dist/",
      "node_modules/",
      "app.html",
      "main.js",
      "main.js.map",
      "package.json"
    ],
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": [
        "deb",
        "AppImage"
      ]
    }
  },
  "directories": {
    "buildResources": "resources",
    "output": "release"
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vinnymac/PokeNurse.git"
  },
  "keywords": [
    "pokemon",
    "go"
  ],
  "author": "duhminick",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vinnymac/PokeNurse/issues"
  },
  "homepage": "https://github.com/vinnymac/PokeNurse#readme",
  "devDependencies": {
    "asar": "0.12.4",
    "babel-core": "6.21.0",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.2.10",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-webpack-loaders": "0.8.0",
    "babel-polyfill": "6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "6.18.0",
    "babili-webpack-plugin": "^0.0.7",
    "cross-env": "3.1.4",
    "css-loader": "0.26.1",
    "del": "^2.2.2",
    "devtron": "^1.3.0",
    "electron": "1.4.13",
    "electron-builder": "10.15.1",
    "electron-devtools-installer": "^2.0.1",
    "eslint": "3.12.2",
    "eslint-config-airbnb": "10.0.1",
    "eslint-formatter-pretty": "^1.1.0",
    "eslint-import-resolver-webpack": "0.8.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "2.2.2",
    "eslint-plugin-no-loops": "^0.3.0",
    "eslint-plugin-react": "6.8.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "2.26.0",
    "jquery": "^2.2.4",
    "json-loader": "^0.5.4",
    "lodash": "4.17.4",
    "minimist": "^1.2.0",
    "moment": "2.17.1",
    "postcss": "5.2.8",
    "react-bootstrap": "^0.30.3",
    "react-burger-menu": "1.10.10",
    "redux-actions": "1.2.0",
    "redux-logger": "^2.6.1",
    "semver": "^5.3.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "1.14.0",
    "webpack-dashboard": "0.2.0",
    "webpack-dev-middleware": "1.9.0",
    "webpack-hot-middleware": "2.15.0",
    "webpack-merge": "2.2.0",
    "webpack-validator": "2.3.0"
  },
  "dependencies": {
    "async-file": "^2.0.2",
    "bootstrap": "^3.3.7",
    "electron-debug": "^1.1.0",
    "electron-localshortcut": "^1.0.0",
    "font-awesome": "^4.7.0",
    "node-pogo-protos": "2.4.2",
    "pogobuf": "1.9.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "5.0.1",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "source-map-support": "0.4.8"
  },
  "devEngines": {
    "node": ">=6.x",
    "npm": ">=3.x"
  }
}
